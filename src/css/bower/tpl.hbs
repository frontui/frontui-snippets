{{#unless className}}
<link rel="stylesheet" href="{{static}}/css/main.css?v={{ version }}">
<script src="{{ bower_components }}/jquery/dist/jquery.min.js?v={{ version }}"></script>
<script src="{{ static }}/js/ui.js"></script>
{{/unless}}
{{#ifCond className '==' 'css'}}
<link rel="stylesheet" href="/bower_components/$  $">
{{/ifCond}}
{{#ifCond className '==' 'js'}}
<script src="/bower_components/$  $" charset="utf-8"></script>
{{/ifCond}}
